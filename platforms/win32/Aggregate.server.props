<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup>
    <_PropertySheetDisplayName>Aggregate Server App</_PropertySheetDisplayName>
    <TargetName>index</TargetName>
    <TargetExt>.app</TargetExt>
  </PropertyGroup>
  <PropertyGroup Label="UserMacros" />
  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>_CRT_SECURE_NO_WARNINGS;COMPILED_FROM_DSP;XML_STATIC;CURL_STATICLIB;CURL_NO_OLDIES;ZLIB_WINAPI;ZLIB;OPENSSL_THREADS;DSO_WIN32;OPENSSL_SYSNAME_WIN32;WIN32_LEAN_AND_MEAN;L_ENDIAN;_CRT_SECURE_NO_DEPRECATE;OPENSSL_BN_ASM_PART_WORDS;OPENSSL_IA32_SSE2;OPENSSL_BN_ASM_MONT;SHA1_ASM;SHA512_ASM;MD5_ASM;RMD160_ASM;AES_ASM;WHIRLPOOL_ASM;OPENSSL_NO_RC5;OPENSSL_NO_MD2;OPENSSL_NO_KRB5;OPENSSL_NO_JPAKE;OPENSSL_NO_DYNAMIC_ENGINE;%(PreprocessorDefinitions)</PreprocessorDefinitions>
      <AdditionalIncludeDirectories>$(SolutionDir)..\..\server\inc;$(SolutionDir)..\..\3rd\libfcgi\inc;$(SolutionDir)..\..\3rd\curl\include;$(SolutionDir)..\..\3rd\libzlib\inc;$(SolutionDir)..\..\3rd\libexpat\inc;$(SolutionDir)..\..\3rd\openssl;$(SolutionDir)..\..\3rd\openssl\tmp32;$(SolutionDir)..\..\3rd\openssl\inc32</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <AdditionalDependencies>ws2_32.lib;crypt32.lib;$(SolutionDir)..\..\bin\$(PlatformName)\$(Configuration)\3rdparty.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <SubSystem>Console</SubSystem>
      <ProgramDatabaseFile>$(TargetDir)$(TargetName).pdb</ProgramDatabaseFile>
      <AdditionalLibraryDirectories>
      </AdditionalLibraryDirectories>
    </Link>
    <PostBuildEvent>
      <Command>call elevated.cmd copy_index.cmd "$(TargetPath)" "..\..\apache\httpd"</Command>
    </PostBuildEvent>
    <PostBuildEvent>
      <Message>Copying to the developement</Message>
    </PostBuildEvent>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>